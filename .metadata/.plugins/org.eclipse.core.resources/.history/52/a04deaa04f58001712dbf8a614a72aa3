package data;

import java.util.List;

import entities.Game;
import entities.Time;

public interface GameDAO {
	public List<Game> index();
	public Game show(int id);
	public Game create(Quiz quiz);
	public Game update(int id, Quiz quiz);
	public boolean destroy(int id);
	public List<Time> showTimeList(int id);
	public Game createGame(int id, Question q);
	public boolean destroyGame(int id, int questid);
}
